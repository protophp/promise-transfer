{
  "name": "protophp/promise-transfer",
  "description": "A mechanism in for reliable transmission data over unreliable long-lived socket connections with delivery, response and resume ability in PHP",
  "keywords": [
    "socket",
    "tcp",
    "unix-socket",
    "delivery",
    "reply",
    "response",
    "resume",
    "php",
    "inter-service-communication",
    "s2s"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Arash Ghamkhari",
      "email": "dev@ghamkhari.com"
    }
  ],
  "require": {
    "opt/opt": "^1.0",
    "protophp/pack": "^0.3",
    "protophp/session": "^1.0",
    "react/socket": "^1.0",
    "monolog/monolog": "^1.0",
    "evenement/evenement": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Proto\\PromiseTransfer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Proto\\PromiseTransfer\\Tests\\": "tests"
    }
  }
}
